{
  "name": "oopsguy/pconfig",
  "description": "A PHP library for parsing configuration(json, yaml, ini, xml and php)",
  "type": "library",
  "license": "MIT",
  "version": "1.1",
  "authors": [
    {
      "name": "Oopsguy",
      "email": "oopsguy@foxmail.com",
      "homepage": "https://github.com/oopsguy/pconfig/graphs/contributors"
    }
  ],
  "support": {
    "issues": "https://github.com/oopsguy/pconfig/issues",
    "source": "https://github.com/oopsguy/pconfig"
  },
  "require": {
    "php": ">=5.6.0",
    "mustangostang/spyc": "0.6.2",
    "ext-json": "*",
    "ext-simplexml": "*",
    "ext-libxml": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2"
  },
  "autoload": {
    "psr-4": {
      "pconfig\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "pconfig\\test\\": "test/"
    }
  }
}
